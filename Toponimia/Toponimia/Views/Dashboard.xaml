<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Toponimia.ViewModels"
             x:Class="Toponimia.Views.Dashboard" Title="Dashboard">

    <ContentPage.BindingContext>
        <local:VM_Dashboard />
    </ContentPage.BindingContext>
    
    <ContentPage.Content>
        <StackLayout>
            
            <Button Text="Gestão" Command="{Binding OpenPageGestao}"></Button>
            <Button Text="Listagem" Command="{Binding OpenPageListagem}"></Button>
            <Button Text="Pesquisas" Command="{Binding OpenPagePesquisa}"></Button>
       
        </StackLayout>
    </ContentPage.Content>
</ContentPage>